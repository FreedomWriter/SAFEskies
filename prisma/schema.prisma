generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "darwin-arm64"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AuthState {
  key   String @id
  state String

  @@map("auth_states")
}

model AuthSession {
  key     String @id
  session String

  @@map("auth_sessions")
}
